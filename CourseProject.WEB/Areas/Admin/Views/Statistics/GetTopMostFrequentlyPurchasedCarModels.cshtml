@model System.Collections.Generic.List<CourseProject.WEB.Models.ModelViewModel>

@{
    ViewBag.Title = "Top models by orders count";
}

<h2>Top models by orders count</h2>

<table class="table">
    <tr>
        <th>@Html.DisplayNameFor(model => model[0].Id)</th>
        <th>@Html.DisplayNameFor(model => model[0].NameWithBrand)</th>
        @*<th>Orders count</th>*@
    </tr>
    @foreach (var item in Model) {
        <tr>
            <td>@item.Id</td>
            <td>@item.NameWithBrand</td>
            @*<td>@item.Cars.Sum(c => c.EquipmentItems.Sum(ei => ei.EquipmentItemValues.Count))</td>*@
        </tr>
    }
</table>
